  "license": "ISC",
  "dependencies": {
    "ionicons": "^5.2.3",
    "notie": "^4.3.1",
    "terser": "^5.3.7"
  },
  "devDependencies": {
    "bulma": "^0.9.1",
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^10.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  }
}
